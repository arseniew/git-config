[core]
        editor = vim
[alias]
        co = checkout
        cm = commit
        st = status
        lb = branch -vv
        lol = log --graph --decorate --pretty=oneline --abbrev-commit --all
        cb = checkout -b
        l = log --pretty='format:%C(yellow)%h %Cblue%>(12)%ad %Cgreen%<(7)%aN%Cred%d %Creset%s'
    
